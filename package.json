{
  "name": "e2e-autotest-boilerplate",
  "version": "0.0.1",
  "description": "CRP E2E autotest boilerplate",
  "scripts": {
    "start": "wdio wdio.conf.js",
    "generate-report": "allure generate --clean _results_/allure-raw",
    "open-report": "allure generate --clean _results_/allure-raw && allure open",
    "generate-suites": "node suites/generator/main.mjs",
    "rename-specs": "node scripts/rename-specs.mjs",
    "delete-old-data": "node scripts/delete-old-data.mjs"
  },
  "keywords": [
    "crp",
    "webdriverio",
    "jasmine",
    "e2e",
    "autotest",
    "aqa",
    "qa"
  ],
  "license": "MIT",
  "devDependencies": {
    "@babel/cli": "7.14.8",
    "@babel/core": "7.14.0",
    "@babel/plugin-proposal-export-default-from": "7.12.13",
    "@babel/preset-env": "7.14.1",
    "@babel/register": "7.13.16",
    "@types/faker": "5.5.4",
    "@types/node": "15.0.2",
    "@wdio/allure-reporter": "7.5.7",
    "@wdio/cli": "7.5.7",
    "@wdio/jasmine-framework": "7.5.3",
    "@wdio/local-runner": "7.5.7",
    "@wdio/selenium-standalone-service": "7.5.7",
    "@wdio/spec-reporter": "7.5.7",
    "allure-commandline": "2.13.8",
    "chromedriver": "99.0.0",
    "dotenv": "9.0.0",
    "faker": "5.5.3",
    "glob": "7.1.7",
    "js-image-generator": "1.0.3",
    "lodash": "4.17.21",
    "minimist": "1.2.5",
    "ts-node": "9.1.1",
    "tsconfig-paths": "3.9.0",
    "typescript": "4.6.2",
    "wdio-chromedriver-service": "7.0.0",
    "webdriverio": "7.5.7"
  }
}
